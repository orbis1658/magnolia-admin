{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "imports": {
    "preact": "https://esm.sh/preact@10.19.6",
    "preact/": "https://esm.sh/preact@10.19.6/",
    "preact-render-to-string": "https://esm.sh/preact-render-to-string@6.2.2"
  },
  "tasks": {
    "build": "deno run --allow-net --allow-read --allow-write --allow-env build.ts",
    "dev": "deno run --allow-net --allow-read --allow-write --allow-env --watch build.ts"
  }
} 